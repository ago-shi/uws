network:
  ethernets:
    eth0:
      dhcp4: false
      addresses: 
        - {{ ansible_host }}/24
      routes:
        - to: default
          via: 192.168.1.1
          metric: 100
      nameservers:
        addresses: 
          - {{ hostvars['dns01']['ansible_host'] }}
        search: [uws.lan]
      dhcp6: false
  version: 2